import java.util.Scanner;
public class Estrutura_Repeticao_Switch_case {

	public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
	int x = sc.nextInt();
	String dia;
	switch (x ) {
	case 1:
		dia = "domingo";
	case 2:
		dia = "segundo";
	case 3:
		dia = "terça";
	case 4:
		dia = "quarta";
	case 5:
		dia = "quinta";
	case 6:
		dia = "sexta";
	case 7:
		dia = "sabado";
	default:
			dia = "valor invalido";
	break;
	}
	System.out.println("Dia da semana :" + dia);
	sc.close();
  }
}
