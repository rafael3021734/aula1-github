package estancias;

public class product {
	public String name;
	public double price;
	public int quantity;
	
	public double totalValueStock(){
		return price * quantity;
	}

	public void addproducts(int quantity) {
		this.quantity += quantity;
	}
	public void removeProducts(int quantity) {
		this.quantity -= quantity;
	}
	public String toString() {
		return name;
	}
}

