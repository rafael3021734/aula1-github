
public class variaveis {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int x, y;
		double z;
		x = 5;
		y = 2* x;
		z = 3* x;
		
	//exemplo 3 calcular da area
		
		double b, B, h, area;
		b = 6.0;
		B = 8.0;
		h = 5.0;
		area = (b+B)/2.0*h;
		System.out.println(x);
		System.out.println(y);
		System.out.println(z);
	    System.out.println(area);
	}
	

}
